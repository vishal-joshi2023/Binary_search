int findMin(vector<int>& arr)
{
	int e = arr.size()-1;
	int s = 0;
	int minEle = INT_MAX;

	while(s<=e){
		int mid = (s+e)/2;

		if(arr[s]<=arr[mid]){
			minEle = min(minEle,arr[s]);
            s = mid+1;
		}
		else{
			minEle = min(minEle,arr[mid]);
			e = mid-1;
		}
	}
	return minEle;
}

// for rotation no -> index 0 till mini ele thats it
